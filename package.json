{
  "name": "phimimms",
  "version": "0.0.0",
  "scripts": {
    "prebuild": "npm-run-all clean:client clean:server && mkdir \"src/client/dist\"",
    "build": "npm-run-all build:client build:server",
    "build:client": "babel-node util/build-client.js",
    "build:server": "rollup --config rollup.config.js",
    "clean": "npm-run-all clean:client clean:docs clean:server clean:test",
    "clean:client": "rimraf src/client/dist",
    "clean:docs": "rimraf docs",
    "clean:server": "rimraf src/server/server.compiled.js",
    "clean:test": "rimraf coverage",
    "dev": "npm run dev:server & npm run dev:client",
    "dev:client": "webpack-dev-server",
    "dev:server": "nodemon src/server/server.js --exec babel-node",
    "predocs": "npm run clean:docs && mkdir \"docs\"",
    "docs": "jsdoc -r src/app -d docs",
    "lint": "eslint src/app",
    "start": "node src/server/server.compiled.js",
    "test": "jest src/app --config jest.json"
  },
  "author": "Philippe Mimms",
  "description": "All Things Philippe Mimms",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "history": "^4.7.2",
    "material-ui": "^0.19.4",
    "moment": "^2.20.1",
    "mongoose": "^4.11.11",
    "perfect-scrollbar": "^1.3.0",
    "prop-types": "^15.5.10",
    "react": "^16.1.1",
    "react-async-component": "^1.0.0-beta.3",
    "react-dom": "^16.1.1",
    "react-localization": "^0.1.3",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.0",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.2.0",
    "tinycolor": "0.0.1",
    "tinygradient": "^0.3.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^3.2.0",
    "eslint": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.8.2",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.5.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-jsdoc": "^3.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jsdoc": "^3.5.5",
    "node-sass": "^4.5.3",
    "nodemon": "^1.14.12",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.1.1",
    "redux-immutable-state-invariant": "^2.0.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.51.8",
    "rollup-plugin-babel": "^3.0.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
}
