{
  "name": "phimimms",
  "version": "0.1.0-alpha",
  "description": "All the small things",
  "scripts": {
    "prebuild": "concurrently \"npm run clean:bin\" \"npm run clean:public\"",
    "build": "concurrently \"npm run build:app\" \"npm run build:svr\"",
    "build:app": "cross-env NODE_ENV=production webpack --mode production",
    "build:svr": "go build -o bin/server src/server/index.go",
    "clean:bin": "rimraf bin && mkdirp bin",
    "clean:public": "rimraf public && mkdirp public",
    "dev": "concurrently \"npm run dev:app\" \"npm run dev:svr\"",
    "dev:app": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "dev:svr": "go run src/server/index.go",
    "docs:app": "typedoc",
    "lint:app": "eslint src/app --ext .svelte --ext .ts",
    "test:app": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://phimimms@github.com/phimimms/phimimms.git"
  },
  "author": "Philippe Mimms",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/phimimms/phimimms/issues"
  },
  "homepage": "https://github.com/phimimms/phimimms#readme",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/uuid": "^3.4.5",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "autoprefixer": "^9.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-svelte3": "^2.6.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mkdirpp": "^1.0.1",
    "node-sass": "^4.13.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "preprocess-loader": "^0.3.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "rimraf": "^2.6.3",
    "svelte": "^3.6.4",
    "svelte-loader": "^2.13.3",
    "svelte-preprocess": "^2.14.4",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typescript": "^3.5.2",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "crypto-hash": "^1.2.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2"
  }
}
